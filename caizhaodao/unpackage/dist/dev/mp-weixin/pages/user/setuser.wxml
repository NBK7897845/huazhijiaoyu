<view><view class="header"><image src="{{form.headerimg}}" mode="aspectFill"></image><view data-event-opts="{{[['tap',[['upimg',['$event']]]]]}}" bindtap="__e">编辑头像</view></view><view style="padding:30rpx;margin-bottom:150rpx;"><view class="tab"><view class="left">昵称</view><view class="right"><input placeholder="请输入" data-name="nickname" data-event-opts="{{[['input',[['inputchange',['$event']]]]]}}" value="{{form.nickname||''}}" bindinput="__e"/></view></view><view class="tab"><view class="left">性别</view><view class="right"><radio-group class="radio-group" data-name="sex" data-event-opts="{{[['change',[['inputchange',['$event']]]]]}}" bindchange="__e"><block wx:for="{{items}}" wx:for-item="item" wx:for-index="index" wx:key="index"><radio class="radio" value="{{item.name}}" checked="{{item.checked}}"><text>{{item.value}}</text></radio></block></radio-group></view></view><view class="tab"><view class="left">出生年月</view><view class="right"><picker mode="date" value="{{form.birthdate}}" start="1900-01-01" end="2020-01-01" data-name="birthdate" data-event-opts="{{[['change',[['inputchange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+form.birthdate+''}}</view></picker></view></view><view class="tab"><view class="left">真实姓名</view><view class="right"><input data-name="realname" placeholder="请输入" data-event-opts="{{[['input',[['inputchange',['$event']]]]]}}" value="{{form.realname||''}}" bindinput="__e"/></view></view><view class="tab"><view class="left">手机号</view><view class="right"><input data-name="mobile" placeholder="请输入" data-event-opts="{{[['input',[['inputchange',['$event']]]]]}}" value="{{form.mobile||''}}" bindinput="__e"/></view></view><view class="tab"><view class="left">地址</view><view class="right"><input data-name="address" placeholder="请输入" data-event-opts="{{[['input',[['inputchange',['$event']]]]]}}" value="{{form.address||''}}" bindinput="__e"/></view></view><view class="tab" style="padding-bottom:250rpx;"><view class="left">职业等级</view><view class="right"><input data-name="occupation" placeholder="请输入" data-event-opts="{{[['input',[['inputchange',['$event']]]]]}}" value="{{form.occupation||''}}" bindinput="__e"/></view></view></view><view data-event-opts="{{[['tap',[['setuser',['$event']]]]]}}" class="towhite" bindtap="__e">修改信息</view></view>